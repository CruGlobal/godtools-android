<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PageContentLayout_Layout">
        <attr name="layout_dynamicHeight" format="boolean" />
        <attr name="layout_childType" format="enum">
            <enum name="hero" value="1" />
            <enum name="card" value="2" />
            <enum name="call_to_action" value="3" />
        </attr>
        <attr name="layout_card_padding_toTopOf" format="reference" />
        <attr name="layout_card_peek_toTopOf" format="reference" />
        <attr name="layout_card_stack_toTopOf" format="reference" />
    </declare-styleable>

    <declare-styleable name="ScaledPicassoImageView">
        <attr name="scaleType" format="enum">
            <enum name="fit" value="0" />
            <enum name="fill" value="1" />
            <enum name="fill_x" value="2" />
            <enum name="fill_y" value="3" />
        </attr>
    </declare-styleable>
</resources>
